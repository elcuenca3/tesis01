{
    "builds": [
        {
            "src": "Aplicativo/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.10.11"
            }
        }
    ],
    "router": [
        {
            "src": "/(.*)",
            "dest": "Aplicativo/wsgi.py"
        }
    ]
}